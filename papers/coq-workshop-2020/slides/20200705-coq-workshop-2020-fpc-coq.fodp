   <draw:page draw:name="page4" draw:style-name="dp4" draw:master-page-name="Alizarin" presentation:presentation-page-layout-name="AL2T1" presentation:use-date-time-name="dtd1">
    <office:forms form:automatic-focus="false" form:apply-design-mode="false"/>
    <draw:frame presentation:style-name="pr17" draw:text-style-name="P19" draw:layer="layout" svg:width="26cm" svg:height="1.875cm" svg:x="1cm" svg:y="0.75cm" presentation:class="title" presentation:user-transformed="true">
     <draw:text-box>
      <text:p><text:span text:style-name="T3">Challenges of tactic languages</text:span></text:p>
     </draw:text-box>
    </draw:frame>
    <draw:frame presentation:style-name="pr18" draw:text-style-name="P22" draw:layer="layout" svg:width="25.5cm" svg:height="9.75cm" svg:x="1.1cm" svg:y="4.125cm" presentation:class="outline" presentation:user-transformed="true">
     <draw:text-box>
      <text:list text:style-name="L4">
       <text:list-item>
        <text:p><text:span text:style-name="T6">Tactic languages as tools to facilitate the </text:span><text:span text:style-name="T7">writing</text:span><text:span text:style-name="T6"> of proofs</text:span></text:p>
       </text:list-item>
      </text:list>
      <text:list text:style-name="L1">
       <text:list-item>
        <text:p><text:span text:style-name="T6">Support for partial proofs and proof search of various kinds</text:span></text:p>
       </text:list-item>
       <text:list-item>
        <text:p><text:span text:style-name="T6">Type inference, existentials, hints, automation...</text:span></text:p>
       </text:list-item>
      </text:list>
      <text:p><text:span text:style-name="T6"/></text:p>
      <text:p xml:id="id11" text:id="id11"><text:span text:style-name="T6">Ideally, such tools should also facilitate the </text:span><text:span text:style-name="T7">maintenance</text:span><text:span text:style-name="T6"> of proofs</text:span></text:p>
      <text:list text:continue-numbering="true" text:style-name="L1">
       <text:list-item>
        <text:p xml:id="id12" text:id="id12"><text:span text:style-name="T6">Ease of writing new tactics, reasoning about their behavior and failure</text:span></text:p>
       </text:list-item>
       <text:list-item>
        <text:p xml:id="id13" text:id="id13"><text:span text:style-name="T6">Tactics as programs are subject to change</text:span></text:p>
       </text:list-item>
       <text:list-item>
        <text:p xml:id="id14" text:id="id14"><text:span text:style-name="T6">Tactics generate proofs that are subject to change</text:span></text:p>
       </text:list-item>
      </text:list>
      <text:p><text:span text:style-name="T6"/></text:p>
      <text:p xml:id="id15" text:id="id15"><text:span text:style-name="T6">Many options: vanilla Ltac, Rtac, Mtac, SsReflect, Ltac2, MetaCoq...</text:span></text:p>
     </draw:text-box>
    </draw:frame>
    <anim:par presentation:node-type="timing-root">
     <anim:seq presentation:node-type="main-sequence">
