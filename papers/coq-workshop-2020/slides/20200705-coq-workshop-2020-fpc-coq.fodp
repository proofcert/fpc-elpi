    <draw:frame presentation:style-name="pr17" draw:text-style-name="P19" draw:layer="layout" svg:width="26cm" svg:height="1.875cm" svg:x="1cm" svg:y="0.75cm" presentation:class="title" presentation:user-transformed="true">
     <draw:text-box>
      <text:p><text:span text:style-name="T3">Our big picture</text:span></text:p>
     </draw:text-box>
    </draw:frame>
    <draw:frame presentation:style-name="pr18" draw:text-style-name="P22" draw:layer="layout" svg:width="25.5cm" svg:height="9.75cm" svg:x="1cm" svg:y="4.125cm" presentation:class="outline" presentation:user-transformed="true">
     <draw:text-box>
      <text:list text:style-name="L4">
       <text:list-item>
        <text:p><text:span text:style-name="T6">Building and maintaining mechanized proofs is complex engineering work</text:span></text:p>
       </text:list-item>
       <text:list-item>
        <text:p><text:span text:style-name="T6"/></text:p>
       </text:list-item>
       <text:list-item>
        <text:p xml:id="id1" text:id="id1"><text:span text:style-name="T6">Tactic languages play a critical role in this </text:span><text:span text:style-name="T7">proof engineering</text:span></text:p>
       </text:list-item>
      </text:list>
      <text:list text:style-name="L1">
       <text:list-item>
        <text:p xml:id="id2" text:id="id2"><text:span text:style-name="T6">Lots of room for experimentation and improvement!</text:span></text:p>
       </text:list-item>
       <text:list-item>
        <text:p xml:id="id3" text:id="id3"><text:span text:style-name="T6">“</text:span><text:span text:style-name="T6">Good” languages should be built to facilitate these tasks</text:span></text:p>
       </text:list-item>
      </text:list>
      <text:list text:style-name="L4">
       <text:list-item>
        <text:p><text:span text:style-name="T6"/></text:p>
       </text:list-item>
       <text:list-item>
        <text:p xml:id="id4" text:id="id4"><text:span text:style-name="T6">We propose to embed and adapt the Foundational Proof Certificates framework to build proofs inside Coq in a clean and user-programmable way</text:span></text:p>
       </text:list-item>
      </text:list>
     </draw:text-box>
    </draw:frame>
