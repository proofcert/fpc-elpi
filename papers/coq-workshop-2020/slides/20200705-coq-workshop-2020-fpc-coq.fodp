     <draw:text-box>
      <text:p><text:span text:style-name="T3">Proof terms and proof scripts</text:span></text:p>
     </draw:text-box>
    </draw:frame>
    <draw:frame presentation:style-name="pr18" draw:text-style-name="P22" draw:layer="layout" svg:width="25.5cm" svg:height="9.75cm" svg:x="1cm" svg:y="4.125cm" presentation:class="outline" presentation:user-transformed="true">
     <draw:text-box>
      <text:list text:style-name="L4">
       <text:list-item>
        <text:p><text:span text:style-name="T6">A proof of a theorem in Coq (i.e., a type) is a term of that type</text:span></text:p>
       </text:list-item>
       <text:list-item>
        <text:p><text:span text:style-name="T6"/></text:p>
       </text:list-item>
       <text:list-item>
        <text:p xml:id="id5" text:id="id5"><text:span text:style-name="T6">Proof terms are usually built indirectly, using </text:span><text:span text:style-name="T8">tactics organized in scripts</text:span></text:p>
       </text:list-item>
       <text:list-item>
        <text:p><text:span text:style-name="T8"/></text:p>
       </text:list-item>
       <text:list-item>
        <text:p xml:id="id10" text:id="id10"><text:span text:style-name="T8">The trusted kernel only needs to typecheck full terms</text:span></text:p>
        <text:p><text:span text:style-name="T8"/></text:p>
       </text:list-item>
       <text:list-item>
        <text:p><text:span text:style-name="T8"/></text:p>
       </text:list-item>
      </text:list>
     </draw:text-box>
    </draw:frame>
    <draw:frame draw:style-name="gr5" draw:text-style-name="P24" draw:layer="layout" svg:width="13.97cm" svg:height="4.826cm" svg:x="0.762cm" svg:y="8.99cm">
     <draw:text-box>
      <text:p><text:span text:style-name="T9">Goal ∀ k m n</text:span><text:span text:style-name="T10">, k + (m + n) = (k + m) + n.</text:span></text:p>
      <text:p><text:span text:style-name="T11">set</text:span><text:span text:style-name="T12"> (P := </text:span><text:span text:style-name="T11">fun k</text:span><text:span text:style-name="T12"> =&gt; </text:span><text:span text:style-name="T9">∀ m n</text:span><text:span text:style-name="T10">, ...).</text:span></text:p>
      <text:p><text:span text:style-name="T9">exact</text:span><text:span text:style-name="T10"> (</text:span><text:span text:style-name="T9">fun k</text:span><text:span text:style-name="T10"> =&gt; nat_ind P</text:span></text:p>
      <text:p><text:span text:style-name="T10"><text:s text:c="2"/></text:span><text:span text:style-name="T10">(</text:span><text:span text:style-name="T9">fun _ _</text:span><text:span text:style-name="T10"> =&gt; eq_refl)</text:span></text:p>
      <text:p><text:span text:style-name="T10"><text:s text:c="2"/></text:span><text:span text:style-name="T10">(</text:span><text:span text:style-name="T9">fun</text:span><text:span text:style-name="T10"> k&apos; (</text:span><text:span text:style-name="T9">IHk</text:span><text:span text:style-name="T10"> : P k&apos;) _ _ =&gt; eq_ind_r</text:span></text:p>
      <text:p><text:span text:style-name="T10"><text:s text:c="5"/></text:span><text:span text:style-name="T10">(</text:span><text:span text:style-name="T9">fun n&apos;</text:span><text:span text:style-name="T10"> =&gt; S n&apos; = _) eq_refl (IHk _ _))</text:span></text:p>
      <text:p><text:span text:style-name="T10"><text:s text:c="2"/></text:span><text:span text:style-name="T10">_). </text:span></text:p>
     </draw:text-box>
    </draw:frame>
    <draw:frame draw:style-name="gr5" draw:text-style-name="P24" draw:layer="layout" svg:width="12.192cm" svg:height="4.826cm" svg:x="15.186cm" svg:y="8.99cm">
     <draw:text-box>
      <text:p><text:span text:style-name="T10"/></text:p>
      <text:p xml:id="id6" text:id="id6"><text:span text:style-name="T10"><text:s text:c="2"/></text:span></text:p>
      <text:p xml:id="id7" text:id="id7"><text:span text:style-name="T9">induction</text:span><text:span text:style-name="T10"> k as [| ? IHk]; </text:span><text:span text:style-name="T9">intros</text:span><text:span text:style-name="T10">. </text:span></text:p>
      <text:p xml:id="id8" text:id="id8"><text:span text:style-name="T10">- </text:span><text:span text:style-name="T9">reflexivity</text:span><text:span text:style-name="T10">.</text:span></text:p>
      <text:p xml:id="id9" text:id="id9"><text:span text:style-name="T10">- </text:span><text:span text:style-name="T9">simpl</text:span><text:span text:style-name="T10">. </text:span><text:span text:style-name="T9">rewrite</text:span><text:span text:style-name="T10"> IHk. </text:span><text:span text:style-name="T9">reflexivity</text:span><text:span text:style-name="T10">.</text:span></text:p>
      <text:p><text:span text:style-name="T10"/></text:p>
     </draw:text-box>
    </draw:frame>
